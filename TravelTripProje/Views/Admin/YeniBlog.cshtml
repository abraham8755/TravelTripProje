@model TravelTripProje.Models.Siniflar.Blog
@{
    ViewBag.Title = "YeniBlog";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>YeniBlog ekleme </h2>
<br/>
<form class="form-group" method="post">
    @Html.LabelFor(x => x.Baslik)
    @Html.TextAreaFor(x => x.Baslik, new { @class = "form-control" })
    <br />
    @Html.LabelFor(x => x.Tarih)
    @Html.TextAreaFor(x => x.Tarih, new { @class = "form-control" })
    <br />
    @Html.LabelFor(x => x.BlokImage)
    @Html.TextAreaFor(x => x.BlokImage, new { @class = "form-control" })
    <br />
    @Html.LabelFor(x => x.Aciklama)
    @Html.TextAreaFor(x => x.Aciklama, new { @class = "form-control", })
    <br />
    <button class="btn btn-primary">Blog Kaydet</button>
</form>

